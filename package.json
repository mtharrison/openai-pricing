{
	"name": "openai-pricing",
	"version": "0.0.0",
	"description": "A package you can use for up to date pricing for OpenAI models",
	"repository": {
		"type": "git",
		"url": "https://github.com/mtharrison/openai-pricing"
	},
	"license": "MIT",
	"author": {
		"email": "hi@matt-harrison.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . .*js --max-warnings 0",
